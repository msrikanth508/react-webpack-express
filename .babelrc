{
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-decorators-legacy",
      "transform-flow-strip-types"
    ],
    "env": {
      "development": {
        "plugins": ["transform-class-properties", "transform-object-rest-spread", "transform-react-remove-prop-types", "transform-react-constant-elements"],
        "presets": ["react-hmre"]
      },
      "production": {
        "plugins": ["transform-class-properties", "transform-object-rest-spread", "transform-react-remove-prop-types", "transform-react-constant-elements"]
      }
    }
  }
  