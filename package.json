{
  "name": "react-webpack-express",
  "description": "Boilerplate for node express react webpack applications",
  "version": "1.0.0",
  "author": "Srikanth Mangipudi <msrikanth508@gmail.com>",
  "keywords": [
    "express",
    "webpack",
    "es6",
    "flow",
    "eslint",
    "node",
    "jade"
  ],
  "engines": {
    "node": "^8 || ^9"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all --parallel lint:watch server",
    "server": "node bin/www",
    "prod": "npm run build && cross-env NODE_ENV=production npm run server",
    "build": "rimraf public/build  && webpack --config=./config/webpack.config.production.js -p",
    "lint": "node_modules/.bin/esw config src",
    "lint:watch": "npm run lint -- --watch"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.12.3",
    "cookie-parser": "1.3.4",
    "debug": "2.1.3",
    "jade": "1.11.0",
    "morgan": "1.5.2",
    "statics": "0.1.0",
    "colors": "1.1.2",
    "compression": "1.7.1",
    "compression-webpack-plugin": "1.1.6",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "empty": "0.10.1",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-flowtype": "2.42.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "flow-bin": "0.64.0",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "run-sequence": "2.2.1",
    "sass-loader": "6.0.6",
    "serve-favicon": "2.4.5",
    "style-loader": "0.20.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0",
    "webpack-merge": "4.1.1",
    "webpack-notifier": "1.5.1"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/msrikanth508/react-webpack-express",
  "repository": {
    "type": "git",
    "url": "git@github.com:msrikanth508/react-webpack-express.git"
  }
}
